{
  "name": "jate",
  "version": "1.0.0",
  "description": "A text editor utilizing PWA criteria.",
  "engines": {
    "node": "19.x",
    "npm": "9.6.2"
  },
  "main": "server.js",
  "scripts": {
    "start:dev": "concurrently \"cd server && npm run server\" \"cd client && npm run dev\"",
    "start": "npm run build && cd server && node server.js",
    "server": "cd server node server.js --ignore client",
    "build": "cd client && npm run build",
    "install": "cd client && npm install",
    "client": "cd client && npm start"
  },
  "keywords": [],
  "author": "Adam Santillana",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "nodemon": "^2.0.4",
    "webpack-cli": "^4.8.0",
    "html-webpack-plugin": "^5.3.2",
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/runtime": "^7.15.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.2.0",
    "http-server": "^0.11.1",
    "style-loader": "^3.2.1",
    "webpack": "^5.51.1",
    "webpack-dev-server": "^4.0.0",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-webpack-plugin": "^6.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Adam42288/pwa-text-editor"
  },
  "bugs": {
    "url": "https://github.com/Adam42288/pwa-text-editor/issues"
  },
  "homepage": "https://github.com/Adam42288/pwa-text-editor#readme"
}
